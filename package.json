{
  "name": "job",
  "version": "1.0.0",
  "description": "Express bootstrap",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon",
    "watch-js": "watchify src/assets/index.ts -v -p tsify -o dist/assets/js/main.js",
    "watch-sass": "node-sass -w src/sass/main.scss dist/assets/css/main.css --include-path node_modules/foundation-sites/scss/ --include-path node_modules/@fortawesome/fontawesome-free/scss/",
    "w": "concurrently -kp \"[{name}]\" -n \"TypeScript,Node,Sass\" -c \"yellow.bold,cyan.bold,red\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-sass\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@types/bcrypt": "^3.0.0",
    "@types/bootstrap": "^4.3.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/express-session": "^1.15.12",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.11",
    "@types/jquery": "^3.3.29",
    "@types/mongoose": "^5.3.26",
    "@types/sharp": "^0.22.2",
    "faker": "^4.1.0",
    "foundation-sites": "^6.5.3",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "ts-jest": "^24.0.2",
    "tsify": "^4.0.1",
    "typescript": "^3.4.4",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.4",
    "express-session": "^1.16.1",
    "jquery": "^3.4.1",
    "mongoose": "^5.5.2",
    "pug": "^2.0.3",
    "sharp": "^0.22.1",
    "what-input": "^5.2.1"
  }
}
